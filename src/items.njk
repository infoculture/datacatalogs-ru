    <p class="description-items">{{ item.description }}</p>

    <p class="items-title">Тема</p>
    <p class="items-p">
        {% for option in item.topics_mult_select %}
            <a class="filter-link" href="/?topic={{topics | getNameById(option)}}">{{option}}</a>
        {% endfor %}
    </p>

    <p class="items-title">Категория</p>
    <p class="items-p">
        {% for option in item.catalog_type %}
            <a class="filter-link" href="/?type={{types | getNameById(option)}}">{{option}}</a>
        {% endfor %}
    </p>

    <p class="items-title">Тип контента</p>
    <p class="items-p">
        {% for option in item.content_type %}
            <a class="filter-link" href="/?type={{types | getNameById(option)}}">{{option}}</a>
        {% endfor %}
    </p>

    <p class="items-title">Регион</p>
    <p class="items-p">
        {% for option in item.country %}
            <a class="filter-link" href="/?content={{contents | getNameById(option)}}">{{option}}</a>
        {% endfor %}
    </p>

    <p class="items-title">Владелец</p>
    <p class="items-p">
         <a class="filter-link" href="{{ item.owner_link }}">{{ item.owner_name }}</a>
    </p>

    <p class="items-title">ПО репозитория</p>
    <p class="items-p">
        {% for option in item.catalog_software %}
            <a class="filter-link" href="/?content={{systems | getNameById(option)}}">{{option}}</a>
        {% endfor %}
    </p>
    <p class="items-title">Объем данных{{dataset_count}}</p>
    <p class="items-p">{{ item.storage_volume }}</p>
    <p class="items-title">Дата внесения</p>
    <p class="items-p">{{ item.add_date | humanDate}}</p>
    <p class="items-title">Дата обновления</p>
    <time datetime="{{item.update_date}}" class="items-p">{{ item.update_date | asPostDate }}</time>

</div>